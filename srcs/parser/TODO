#define LOG(str) dbg_logger(str, __LINE__, __FILE__, __FUNCTION__)

void    dbg_logger(char *str, int line, char *file, char *func);

void    dbg_logger(char *str, int line, char *file, char *func)
{
	ft_printf("%s at %s, function %s:%d\n", str, file, func, line);
}